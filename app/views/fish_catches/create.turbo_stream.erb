<%= turbo_stream.update 'stats' do %>
    <%= render "tackle_box_items/stats", fish_catches: @fish_catches %>
<% end %>

<%= turbo_stream.update 'fish_catch_form' do %>
    <%= render "form", fish_catch: @new_catch %>
<% end %>

<%= turbo_stream.prepend 'fish_catches' do %>
    <%= render @fish_catch %>
<% end %>